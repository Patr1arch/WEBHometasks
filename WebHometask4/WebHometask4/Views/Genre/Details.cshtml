@model WebHometask4.ViewModels.GenreModel
@{
    ViewData["Title"] = "Жанр в деталях";
}

<h2> Подробное описание жанра</h2>
<table class="table-condensed">
    <tr>
        <td> Id </td>
        <td> Название</td>
        <td> С создания прошло</td>
    </tr>
    <tr>
        <td>
            @Model.Id
        </td>
        <td>
            @Model.Name
        </td>
        <td> 
            @Model.Lifetime
        </td>
    </tr>
</table>
<button class="btn btn-primary" onclick="location.href = '/Genre/GenreNames';"> Вернуться в список </button>