@model WebHometask4.ViewModels.CompanyModel
@{
    ViewData["Title"] = "Компания в деталях";
}

<h2> Подробное описание компании</h2>
<table class="table-condensed">
    <tr>
        <td> Id </td>
        <td> Название</td>
        <td> С создания прошло</td>
    </tr>
    <tr>
        <td>
            @Model.Id
        </td>
        <td>
            @Model.Name
        </td>
        <td>
            @Model.Lifetime
        </td>
    </tr>
</table>
<button class="btn btn-primary" onclick="location.href = '/Company/CompanyNames';"> Вернуться в список </button>